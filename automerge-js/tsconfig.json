{
  "compilerOptions": {
    "noImplicitAny": false,
    "strict": true,
    "allowJs": false,
    "baseUrl": ".",
    "lib": ["dom", "esnext.asynciterable", "es2017", "es2016", "es2015"],
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es2016",
    "skipLibCheck": true,
    "outDir": "./dist"
  },
  "include": [ "src/**/*" ],
  "exclude": ["dist/**/*"]
}
